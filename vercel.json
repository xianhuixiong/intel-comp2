{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/assets/(.*)\\.js",
      "headers": [{ "key": "Content-Type", "value": "text/javascript; charset=utf-8" }]
    },
    {
      "source": "/assets/(.*)\\.css",
      "headers": [{ "key": "Content-Type", "value": "text/css; charset=utf-8" }]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
